{
  "name": "Generador de Reels Autom√°tico v2",
  "nodes": [
    {
      "id": "00000000-0000-0000-0000-000000000001",
      "name": "Trigger Manual",
      "type": "@n8n/n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [100, 200],
      "parameters": {}
    },
    {
      "id": "00000000-0000-0000-0000-000000000002", 
      "name": "Generar Script",
      "type": "@n8n/n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [300, 200],
      "parameters": {
        "method": "POST",
        "url": "https://api.openai.com/v1/chat/completions",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Authorization",
              "value": "Bearer YOUR_OPENAI_API_KEY"
            },
            {
              "name": "Content-Type", 
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "model",
              "value": "gpt-4"
            },
            {
              "name": "messages",
              "value": [
                {
                  "role": "system",
                  "content": "Eres un experto en crear scripts virales para reels de Instagram. Crea contenido atractivo y viral."
                },
                {
                  "role": "user", 
                  "content": "Crea un script para un reel viral sobre {{ $json.topic || 'tendencias de marketing digital' }}"
                }
              ]
            },
            {
              "name": "max_tokens",
              "value": 500
            }
          ]
        }
      }
    },
    {
      "id": "00000000-0000-0000-0000-000000000003",
      "name": "Generar Audio",
      "type": "@n8n/n8n-nodes-base.httpRequest", 
      "typeVersion": 4.2,
      "position": [500, 200],
      "parameters": {
        "method": "POST",
        "url": "https://api.elevenlabs.io/v1/text-to-speech/voice-id",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "xi-api-key",
              "value": "YOUR_ELEVENLABS_API_KEY"
            }
          ]
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "text",
              "value": "={{ $node['Generar Script'].json.choices[0].message.content }}"
            },
            {
              "name": "voice_settings",
              "value": {
                "stability": 0.5,
                "similarity_boost": 0.75
              }
            }
          ]
        }
      }
    },
    {
      "id": "00000000-0000-0000-0000-000000000004",
      "name": "Crear Video",
      "type": "@n8n/n8n-nodes-base.httpRequest",
      "typeVersion": 4.2, 
      "position": [700, 200],
      "parameters": {
        "method": "POST",
        "url": "https://api.runway.ml/v1/generate",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Authorization",
              "value": "Bearer YOUR_RUNWAY_API_KEY"
            }
          ]
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "prompt",
              "value": "={{ $node['Generar Script'].json.choices[0].message.content }}"
            },
            {
              "name": "duration": 15,
              "value": "aspect_ratio"
            },
            {
              "name": "aspect_ratio",
              "value": "9:16"
            }
          ]
        }
      }
    },
    {
      "id": "00000000-0000-0000-0000-000000000005",
      "name": "Publicar en Instagram",
      "type": "@n8n/n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [900, 200],
      "parameters": {
        "method": "POST",
        "url": "https://graph.facebook.com/v18.0/{{ $vars.INSTAGRAM_USER_ID }}/media",
        "authentication": "genericCredentialType", 
        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Authorization",
              "value": "Bearer YOUR_INSTAGRAM_ACCESS_TOKEN"
            }
          ]
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "video_url",
              "value": "={{ $node['Crear Video'].json.video_url }}"
            },
            {
              "name": "caption",
              "value": "={{ $node['Generar Script'].json.choices[0].message.content }}"
            },
            {
              "name": "media_type",
              "value": "REELS"
            }
          ]
        }
      }
    }
  ],
  "connections": {
    "Trigger Manual": {
      "main": [
        [
          {
            "node": "Generar Script",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Generar Script": {
      "main": [
        [
          {
            "node": "Generar Audio",
            "type": "main", 
            "index": 0
          }
        ]
      ]
    },
    "Generar Audio": {
      "main": [
        [
          {
            "node": "Crear Video",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Crear Video": {
      "main": [
        [
          {
            "node": "Publicar en Instagram",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": false,
  "settings": {
    "executionOrder": "v1"
  },
  "staticData": {},
